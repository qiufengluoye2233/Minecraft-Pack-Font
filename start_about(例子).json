{
    "namespace": "start_about",
    "Loli_panel": {
        "type": "stack_panel",
        "anims": [
            "@setting_anim.screen_entrance_size_animation_push",
            "@setting_anim.screen_entrance_size_animation_pop",
            "@setting_anim.about_exit_size_animation_pop",
            "@setting_anim.about_exit_size_animation_push"
        ],
        "size": [
            "80%",
            "85%"
        ],
        "orientation": "vertical",
        "controls": [
            {
                "header@start_about.header": {
                    "layer": 4
                }
            },
            {
                "panel@start_about.panel5": {
                    "size": [
                        "100%",
                        "90%"
                    ]
                }
            }
        ]
    },
    "header": {
        "type": "panel",
        "size": [
            "100%",
            20
        ],
        "anchor_from": "top_middle",
        "anchor_to": "top_middle",
        "controls": [
            {
                "header_background": {
                    "type": "image",
                    "alpha": 0.8,
                    "texture": "textures/ui/header_bar_2"
                }
            },
            {
                "legacy_pocket_close_button@common.legacy_pocket_close_button": {
                    "offset": [
                        -3,
                        2
                    ]
                }
            }
        ]
    },
    "panel5": {
